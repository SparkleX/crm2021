{
	"filterItems": [],
	"columns": {
		"name": {
			"label": {
				"Type": "sap.m.Label",
				"text": "Name"
			},
			"template": {
				"Type": "sap.nsme.share.widget.Text",
				"text": "{name}",
				"dataBind": "ResponseOption.name"
			}
		},
		"classify": {
			"hAlign": "Center",
			"label": {
				"Type": "sap.m.Label",
				"text": "Classification"
			},
			"template": {
				"Type": "sap.nsme.share.widget.Text",
				"text": "{classify}",
				"dataBind": "ResponseOption.classify"
			}
		},
		"status": {
			"hAlign": "Center",
			"label": {
				"Type": "sap.m.Label",
				"text": "Status"
			},
			"template": {
				"Type": "sap.nsme.share.widget.Text",
				"text": "{status}",
				"dataBind": "ResponseOption.status"
			}
		},
		"genLeads":{
			"hAlign": "Center",
			"label": {
				"Type": "sap.m.Label",
				"text": "Generate Leads"
			},
			"template": {
				"Type": "sap.m.CheckBox",
				"selected": "{=${genLeads}==='Y'}",
				"displayOnly": true,
				"dataBind": "ResponseOption.genLeads"
			}			
		}
	},
	"columnOrder": [
		"name",
		"status",
		"classify",
		"genLeads"
	]
}