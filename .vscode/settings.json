{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"search.useIgnoreFiles": false,
	"search.exclude": {"dist": true},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.associations": {
		"*.bo": "json",
		"*.api": "json"
	},
	"json.schemas": [
		{
			"fileMatch": ["*.table.json"],
			"url": "./resources/schema/table/index.json"
		}
	]
}
